{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "valid-typeof": "warn"
    }
}