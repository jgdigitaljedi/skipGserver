{
	"env": {
		"browser": false,
		"mongo": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"valid-typeof": "warn",
		"eqeqeq": "warn",
		"semi": [ "warn, always" ]
	}
}
